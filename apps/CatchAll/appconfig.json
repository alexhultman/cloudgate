{
  "env": "PROD",
  "version": "1.0.0",
  "title": "Catch All App",
  "description": "This is a sample app that catch all domains not configured",
  "domains": ["*", "toto3.vms2.terasp.net", "toto4.vms2.terasp.net", "toto5.vms2.terasp.net", "*.vms2.terasp.net"], 
  "publicFolder": "./public",
  "TypeAPI": "LOCAL",
  "TypeFS": "LOCAL",
  "redirect404toIndex": false,
  "HSTS": false,
  "maxRequestsPerMinutePerIP": 120,
  "CORS": {
      "access-control-allow-origin": "*"
  },
  "apiEndpoints": {
    "/tests/simple" : {
      "src": "./API/tests/",
      "handler": "simple.handler"
    },
    "/tests/full" : {
      "src" : "./API/tests/",
      "handler": "full.handler",
      "maxRequestsPerMinutePerIP": 50
    },
    "/wildcardtest/*" : {
      "src" : "./API/tests/",
      "handler": "full.handler"
    }
  },
  "websocketEndpoints": {
    "/echo" : {
      "src" : "./API/websocket/",
      "upgrade": "Echo.upgrade",
      "open": "Echo.open",
      "message": "Echo.message",
      "close": "Echo.close"
    },
    "/chat" : {
      "src" : "./API/websocket/",
      "open": "Chat.open",
      "message": "Chat.message",
      "close": "Chat.close"
    }
  }
}
